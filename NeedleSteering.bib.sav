@Article{Elgezua2013,
  author   = {Inko Elgezua and Yo Kobayashi and Makatsu G. Fujie},
  journal  = {Procedia CIRP},
  title    = {Survey on Current State-of-the-Art in Needle Insertion Robots: Open Challenges for Application in Real Surgery},
  year     = {2013},
  issn     = {2212-8271},
  note     = {First CIRP Conference on BioManufacturing},
  pages    = {94 - 99},
  volume   = {5},
  abstract = {Minimally invasive percutaneous treatment robots have become a popular area in medical robotics. Minimally invasive treatments are an important part of modern surgery; however percutaneous treatments are a difficult procedure for surgeons. They must carry out a procedure that has limited visibility, tool maneuverability and where the target and tissue surrounding it move because of the tool. Robot technology can overcome those limitations and increase the success of minimally invasive percutaneous treatment. In this paper we will present a review of the current state-of-the-art in robotic insertion needle for minimally invasive treatments, focusing on the limitations and challenges still open for their use in clinical application.},
  doi      = {https://doi.org/10.1016/j.procir.2013.01.019},
