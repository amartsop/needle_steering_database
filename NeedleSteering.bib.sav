  file     = {:Model/FrictionModels/johnson1992.pdf:PDF},
  groups   = {Friction Models, Next},
  keywords = {compensation;feedback;friction;manipulators;nonlinear control systems;position control;position controlled mechanisms;nonlinear friction;closed-loop controller;positioning accuracy;iterative signal processing;robotic gripper;preloaded rack-and-pinion mechanism;Friction;Robotics and automation;Damping;Predictive models;Industry Applications Society;Iterative methods;Signal processing;Position control;Signal resolution;Robots},
}

