@Article{1461405,
  author   = {S. {Okazawa} and R. {Ebrahimi} and J. {Chuang} and S. E. {Salcudean} and R. {Rohling}},
  title    = {Hand-held steerable needle device},
  journal  = {IEEE/ASME Transactions on Mechatronics},
  year     = {2005},
  volume   = {10},
  number   = {3},
  pages    = {285-296},
  month    = {June},
  issn     = {1941-014X},
  abstract = {Minimally invasive percutaneous medical procedures are widely accepted in current clinical practice. However, with current techniques, the procedures are challenging and a high level of expertise is required to perform them successfully. This paper presents a novel steerable needle device for percutaneous interventions that allows the physician to steer the tip of the needle during insertion which eases the challenge associated with reaching the target. The steering concept uses slightly modified, off-the-shelf medical biopsy needles. A controlled lateral steering motion of over 30 mm during a 100-mm needle insertion is demonstrated with a 20-gauge needle in tissue-mimicking phantoms. A hand-held, motorized device has been built that actuates the needle base to produce a desired steering direction and magnitude at the tip. Steering commands may be supplied either by user input or by computer control. For the latter case, a method of software path planning has been developed that automatically steers the needle to a target.},
  doi      = {10.1109/TMECH.2005.848300},
  file     = {:Model/MecanicsBasedModels/lehmann2015.pdf:PDF},
  groups   = {Mechanics Based Models},
  keywords = {surgery;medical robotics;steering systems;needles;motion control;control engineering computing;path planning;hand held steerable needle device;minimally invasive percutaneous medical procedure;off the shelf medical biopsy needle;lateral steering motion control;computer control;software path planning;Needles;Medical diagnostic imaging;Biomedical imaging;Biopsy;Mechanical engineering;Skin;Surgery;Motion control;Imaging phantoms;Automatic control;Biopsy;medical robotics;needle;steerable},
}

@InProceedings{1014848,
  author    = {C. {Simone} and A. M. {Okamura}},
  title     = {Modeling of needle insertion forces for robot-assisted percutaneous therapy},
  booktitle = {Proceedings 2002 IEEE International Conference on Robotics and Automation (Cat. No.02CH37292)},
  year      = {2002},
  volume    = {2},
  pages     = {2085-2091 vol.2},
  month     = {May},
  abstract  = {Force information from needle insertions was measured and modeled for use in robot-assisted percutaneous therapies. Data was collected on bovine livers using the Johns Hopkins University Steady Hand Robot, and modeled in three parts: force due to capsule stiffness, friction, and cutting. Capsule stiffness is modeled by a nonlinear spring model, friction by a modified Karnopp model, and cutting by the remaining forces, which appear to be constant for a given tissue sample. During robot-assisted procedures, real-time force data can be compared to these models to control puncture of interior structures.},
  doi       = {10.1109/ROBOT.2002.1014848},
  file      = {:Model/MecanicsBasedModels/Modeling_of_Needle_Insertion_Forces_for_Robot-Assi.pdf:PDF},
  groups    = {Mechanics Based Models},
  keywords  = {medical robotics;friction;biological tissues;patient treatment;patient diagnosis;haptic interfaces;needle insertion forces;robot-assisted percutaneous therapy;bovine livers;Johns Hopkins University Steady Hand Robot;capsule stiffness;friction;cutting;nonlinear spring model;modified Karnopp model;real-time force data;puncture;tissue layers;liver ablation;prostate brachytherapy;Needles;Robots;Medical treatment;Robotics and automation;Haptic interfaces;Instruments;Minimally invasive surgery;Humans;Feedback;Force measurement},
}

