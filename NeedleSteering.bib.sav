  file      = {:Model/Identification/00380769.pdf:PDF},
  groups    = {Identification},
  keywords  = {telerobotics;mechanoception;feedback;interactive devices;finger touch feedback;perceptual feedback issues;dexterous telemanipulation;touch display technologies;neuromuscular stimulations;pneumatics approach;air jets;air pockets;inflatable bladders;touch feedback cues;vibro-tactile approach;vibrating pins;piezoelectric crystals;tickling sensation;electro-tactile stimulation method;electric pulses;sensory substitution;touch feedback devices;Telerobotics;Tactile system;Output feedback;Computer input/output},
}

@InProceedings{Matsumiya2003,
  author    = {Matsumiya, Kiyoshi and Momoi, Yasuyuki and Kobayashi, Etsuko and Sugano, Nobuhiko and Yonenobu, Kazuo and Inada, Hiroshi and Tsuji, Takayuki and Sakuma, Ichiro},
  booktitle = {Medical Image Computing and Computer-Assisted Intervention - MICCAI 2003},
  title     = {Analysis of Forces during Robotic Needle Insertion to Human Vertebra},
  year      = {2003},
  address   = {Berlin, Heidelberg},
  editor    = {Ellis, Randy E. and Peters, Terry M.},
  pages     = {271--278},
  publisher = {Springer Berlin Heidelberg},
  abstract  = {The purpose of this study is assessment of forces and torques required for robotic needle insertion to human vertebra. Axial forces during robotic insertion to human vertebrae fixed with formalin (water solution of formaldehyde(FA)) did not exceed 25 N when the feed rate was no more than 0.5 mm/s. There was relatively strong correlation between the axial force variation during insertion and the distribution of bone local CT-value along the needle path. This result shows the importance of preoperative X-ray CT image guidance for robotic needle insertion with safely small axial forces. There was no significant influence of FA-fixation on axial forces during robotic needle insertion to human femoral heads. The forces during robotic insertion to human femoral head were as small as less than 50{\%} of those during manual insertion, and which indicates that the robot can contribute to the safe needle insertion to human vertebra in percutaneous vertebroplasty.},
  file      = {:Model/Identification/Matsumiya2003_Chapter_AnalysisOfForcesDuringRoboticN.pdf:PDF},
  groups    = {Axial Forces, C2 Data, Next},
  isbn      = {978-3-540-39899-8},
}

@InProceedings{Naemura2008,
  author    = {K. {Naemura} and A. {Sakai} and T. {Hayashi} and H. {Saito}},
