@InProceedings{Gordon2015,
  author    = {Adam Gordon and Barnett, {Andrew C.} and Inki Kim and Moore, {Jason Zachary}},
  title     = {Needle insertion force model for haptic simulation},
  booktitle = {Materials; Biomanufacturing; Properties, Applications and Systems; Sustainable Manufacturing},
  year      = {2015},
  series    = {ASME 2015 International Manufacturing Science and Engineering Conference, MSEC 2015},
  month     = {1},
  publisher = {American Society of Mechanical Engineers},
  note      = {ASME 2015 International Manufacturing Science and Engineering Conference, MSEC 2015 ; Conference date: 08-06-2015 Through 12-06-2015},
  abstract  = {Percutaneous medical procedures rely upon clinicians performing precise needle insertion in soft tissue. The utility of haptic simulation systems in training clinicians for these procedures is highly dependent upon the ability to render accurate insertion force feedback. This paper presents a piecewise mathematical model for insertion force that does not require tissue material properties, detailed mechanical approximations, or complex computations. With manipulation of model parameters, a wide variety of insertion tasks and clinical scenarios can be modeled. Through needle insertion experiments and parameter estimation, this model was demonstrated to replicate the insertion forces associated with a variety of needle and tissue types. In 11 of 12 needle and tissue combinations tested, the model replicated the insertion force with an average absolute mean error of less than 0.065 N.},
  day       = {1},
  doi       = {10.1115/MSEC20159352},
  file      = {:Model/Identification/gordon2015.pdf:PDF},
  groups    = {Identification, Axial Forces},
  keywords  = {rank5},
  language  = {English (US)},
}

